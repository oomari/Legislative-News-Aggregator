// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model NewsArticle {
  id         Int      @id @default(autoincrement())
  title      String
  publishedAt DateTime
  state      String
  description String?
  articleUrl String
}